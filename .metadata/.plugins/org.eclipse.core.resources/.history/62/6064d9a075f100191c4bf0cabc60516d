package DataStructures;

import Problems.CheckPalindrome;

public class TestLL {
	
	public static void main(String[] args)
	{
		LinkedList l = new LinkedList();
		l.append(10);
		l.append(20);
		l.append(30);
		l.append(30);
		l.append(20);
		l.append(10);
		l.append(40);
		
		l.print();
		
		l.delete(40);
		System.out.println("---");
		l.print();
		
		boolean res = CheckPalindrome.isPalindrome(l);
		System.out.println(res);
	}

}
